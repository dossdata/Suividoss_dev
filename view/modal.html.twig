<!--statbilan par CDM -->
<div class="modal fade" id="bilan_faitpar_cdm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content" style="width:100%;position:fixed;top:0;left:0;overflow:auto;height:850px;zoom:90%">
      <div class="modal-header bg-info">
        <h4 class="modal-title w-100" id="myModalLabel">Bilan fait par Cdm et Reviser par Reviseur</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body text-center">
        <script type="text/javascript">
          var rep = "";
          var remois = "";
          var dtms = "";
          for (var i = 0; i < 32; i++) {
            rep += "<option value ='" + (
            2000 + i) + "'>" + (
            2000 + i) + "</option>";
            if (i > 0 && i < 13) {
              if (i == 1) {
                dtms = "Janvier";
              }
              if (i == 2) {
                dtms = "Fevier";
              }
              if (i == 3) {
                dtms = "Mars";
              }
              if (i == 4) {
                dtms = "Avril";
              }
              if (i == 5) {
                dtms = "Mai";
              }
              if (i == 6) {
                dtms = "Juin";
              }
              if (i == 7) {
                dtms = "Juillet";
              }
              if (i == 8) {
                dtms = "Aout";
              }
              if (i == 9) {
                dtms = "Septembre";
              }
              if (i == 10) {
                dtms = "Octobre";
              }
              if (i == 11) {
                dtms = "Novembre";
              }
              if (i == 12) {
                dtms = "Decembre";
              }
              if (i < 10) {
                remois += "<option value ='0" + (
                i) + "'>" + (
                dtms) + "</option>";
              } else {
                remois += "<option value ='" + (
                i) + "'>" + (
                dtms) + "</option>";
              }
            }

          }
          document.write("exercice <select id='anneebilan' ><option></option>" + rep + "</select> <select id='moisbilan' ><option></option>" + remois + "</select> <button class='btn-primary' id='statparcdm'>Valider</button>");
        </script>
      </div>
      <ul style="position:relative;right:2%;bottom:13%">
        <div class="text-center" id="loading2" style="position:absolute;top:35%;left:45%;display:none"><img src="./asset/images/load.gif"></div>
        <li test="test" style="display: inline-block;overflow:auto;height:563px;width:25%">
          <table id="blfaitparcdm" class="table-bordered text-center" width="100%">
            <thead>
              <tr>
                <th style="display:none">id</th>
                <th>CDM MADA</th>
                <th>TOTAL FAIT</th>
                <th>TOTAL FAIT AUTRE</th>
              </tr>
            </thead>
            <tbody >
              {% for cdm in gasy %}
                <tr>
                  <td class="id_user_mada" style="display:none">{{cdm.id}}</td>
                  <td>{{cdm.nom}}</td>
                  <td class="nbf"></td>
                  <td class="nbfautre"></td>
                </tr>
              {% endfor %}
            </tbody>
          </table>
        </li>

        <li style="display: inline-block;overflow:auto;height:563px;width:20%">
          <table id="tablefr" class="table-bordered text-center" width="100%">
            <thead>
              <thead>
                <tr>
                  <th style="display:none">id</th>
                  <th>CDM FRANCE</th>
                  <th>TOTAL FAIT</th>
                  <th>TOTAL FAIT AUTRE</th>
                </tr>
              </thead>
            </thead>
            <tbody>

              {% for cdm in france %}
                <tr>
                  <td class="id_user_france" style="display:none">{{cdm.id}}</td>
                  <td>{{cdm.nom}}</td>
                  <td></td>
                  <td></td>
                </tr>
              {% endfor %}
            </tbody>
          </table>
        </li>

        <li test="test" style="display: inline-block;overflow:auto;height:563px;width:25%">
          <table id="blfaitparrevmada" class="table-bordered text-center" width="100%">
            <thead>
              <tr>
                <th style="display:none">id</th>
                <th>REVISEUR MADA</th>
                <th>TOTAL FAIT</th>
                <th>TOTAL FAIT AUTRE</th>
              </tr>
            </thead>
            <tbody >
              {% for rev in gasy_rev %}
                <tr>
                  <td class="id_user_rev" style="display:none">{{rev.id}}</td>
                  <td>{{rev.nom}}</td>
                  <td class="nbf_rev"></td>
                  <td class="nbfautre_rev"></td>
                </tr>
              {% endfor %}
            </tbody>
          </table>
        </li>

        <li style="display: inline-block;overflow:auto;height:563px;width:20%">
          <table id="tablefrrevfrance" class="table-bordered text-center" width="100%">
            <thead>
              <thead>
                <tr>
                  <th style="display:none">id</th>
                  <th>REVISEUR FRANCE</th>
